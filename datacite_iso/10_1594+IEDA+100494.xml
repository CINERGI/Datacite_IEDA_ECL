            <gmd:CI_Citation>
               <gmd:title>
                  <gco:CharacterString>Effects of Mineral Spatial Patterns on Magnesite Dissolution Rates</gco:CharacterString>
               </gmd:title>
               <gmd:date>
                  <gmd:CI_Date>
                     <gmd:date>
                        <gco:Date>2015-11-15</gco:Date>
                     </gmd:date>
                     <gmd:dateType>
