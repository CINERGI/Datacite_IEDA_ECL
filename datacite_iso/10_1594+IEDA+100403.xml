               <gmd:title>
                  <gco:CharacterString>RU_CAGeochem v.2, a database and sample repository for Central American volcanic rocks at Rutgers University</gco:CharacterString>
               </gmd:title>
               <gmd:date>
                  <gmd:CI_Date>
                     <gmd:date>
                        <gco:Date>2013-08-15</gco:Date>
                     </gmd:date>
                     <gmd:dateType>
